{
  "$schema" : "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": ""
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Global": {
        "TimeOut": "9999:00:00"
      },
      "Security": {
        "KeepUserLoggedIn": true
      }
    }
  },
  "Formulate": {
    "Templates": {
      "Items": [
        {
          "Id": "F3FB1485C1D14806B4190D7ABDE39530",
          "Name": "Plain JavaScript",
          "Path": "~/Views/Partials/Formulate/Responsive.Plain JavaScript.cshtml"
        }
      ]
    }
  }
}
